{
  "name": "ewidencja-pracy-warsztatu",
  "version": "1.0.4",
  "private": true,
  "author": "Pawel Naworol",
  "nodeGypRebuild": "true",
  "description": "Aplikacja systemowa zarządzająca Ewidencją pracy warsztatu",
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-preset-react-app": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^0.26.4",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^0.3.0",
    "electron-packager": "^12.1.1",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "foreman": "^2.0.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "17.0.2",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^1.0.0",
    "promise": "7.1.1",
    "react-dev-utils": "^0.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-to-print": "^2.0.0-alpha-2",
    "recursive-readdir": "2.1.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "strip-ansi": "3.0.1",
    "style-loader": "^0.13.2",
    "styled-components": "^3.3.3",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@react-pdf/renderer": "^1.0.0-alpha.14",
    "classnames": "^2.2.6",
    "electron-is-dev": "^0.3.0",
    "html2canvas": "^1.0.0-alpha.12",
    "jspdf": "^1.4.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-html-table-to-excel": "^2.0.0",
    "react-redux": "^5.0.7",
    "recompose": "^0.28.2",
    "redocx": "^1.0.3",
    "redux": "^4.0.0",
    "typeface-open-sans": "0.0.54",
    "typeface-roboto": "0.0.54"
  },
  "homepage": "./",
  "main": "./public/electron.js",
  "build": {
    "appId": "oneoften",
    "files": [
      "./build/**/*",
      "./public/electron.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "1": "electron-builder install-app-deps",
    "2": "electron-builder node-gyp-rebuild  ",
    "3": "electron-builder create-self-signed-cert  ",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "win": "electron-builder --win --x64 --dir",
    "dev": "nf start -p 3000",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "postinstall": "electron-builder install-app-deps"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
